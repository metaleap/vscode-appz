{
	"name": "vscode-appz",
	"displayName": "VS Code Appz",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.37.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
